{
  "name": "app-center",
  "env": {
    "HEROKU_APP_NAME": {
      "required": true
    },
    "AWS_ACCESS_KEY_ID": {
      "required": false
    },
    "AWS_SECRET_KEY": {
      "required": false
    },
    "AWS_BUCKET": {
      "required": false
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "addons": ["mongolab"],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    { "url": "https://github.com/heroku/heroku-buildpack-static" }
  ],
  "stack": "heroku-18"
}
